{
    "name": "bpocallaghan/titan-starter",
    "type": "project",
    "description": "A Laravel Website with Admin access Starter project with AdminLTE theme and basic features.",
    "keywords": [
        "cms",
        "admin",
        "titan",
        "website",
        "laravel",
        "framework",
        "bpocallaghan"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "bpocallaghan/alert": "^1.1",
        "bpocallaghan/impersonate": "^1.1",
        "bpocallaghan/log-activity": "1.*",
        "bpocallaghan/notify": "^3.0",
        "bpocallaghan/sluggable": "^1.3",
        "brian2694/laravel-toastr": "^5.56",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "imanghafoori/laravel-video": "^2.1",
        "intervention/image": "^2.5",
        "lakshmaji/thumbnail": "^1.4",
        "laravel/framework": "^8.12",
        "laravel/ui": "^3.1",
        "laravelcollective/html": "^6.3",
        "php-ffmpeg/php-ffmpeg": "^0.19.0",
        "pion/laravel-chunk-upload": "^1.4",
        "spatie/laravel-analytics": "^3.6",
        "tymon/jwt-auth": "^1.0",
        "wester/chunk-upload": "^2.4",
        "yajra/laravel-datatables-oracle": "~9.0",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "bpocallaghan/generators": "^7.0.1",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/tinker": "^2.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/CustomHelper.php"
        ]   
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/CustomHelper.php"
        ]  
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
